{
  "name": "optical-surveys-2009",
  "title": "Optical Surveys (2009)",
  "description": "Positions of an Ohio State University GPS receiver on the glacier main branch measured by a robotic survey theodolite.",
  "version": "0.1.0",
  "contributors": [
    {
      "title": "Ethan Welty",
      "email": "ethan.welty@gmail.com",
      "role": "author"
    },
    {
      "title": "Shad O'Neel",
      "role": "Performed the research and processed the data."
    }
  ],
  "sources": [
    {
      "name": "Original data, scripts, and documentation",
      "path": "sources/"
    }
  ],
  "resources": [
    {
      "name": "markers",
      "path": "data/markers.csv",
      "schema": {
        "fields": [
          {
            "name": "t",
            "type": "datetime",
            "description": "Date and time (UTC).",
            "format": "%Y-%m-%dT%H:%M:%SZ"
          },
          {
            "name": "x",
            "type": "number",
            "description": "Easting in WGS84 UTM Zone 6N (EPSG:32606).",
            "unit": "m"
          },
          {
            "name": "y",
            "type": "number",
            "description": "Northing in WGS84 UTM Zone 6N (EPSG:32606).",
            "unit": "m"
          },
          {
            "name": "z",
            "type": "number",
            "description": "Height above the WGS84 ellipsoid.",
            "unit": "m"
          }
        ]
      }
    }
  ]
}
